{
    "installCommand": "npm install",
    "version": 2,
    "buildCommand": "npm run build",
    "builds": [
        {
            "src": "src/server.js",
            "use": "@vercel/node"
        },
        {
            "src": "src/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}